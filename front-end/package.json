{
  "name": "5-seen-back-end",
  "version": "0.1.0",
  "description": "5-Seen-Wanderung-Spiel Back-End. Websocket fuer Spiel-Synchronisation zwischen Front-Ends.",
  "main": "index.html",
  "scripts": {
    "clean": "rimraf build",
    "babel": "./node_modules/.bin/babel build-tsc --out-dir build-babel --source-maps",
    "webpack": "webpack --config webpack.config.jscript && npm run postbuild",
    "clean-build-steps": "rimraf build-tsc build-babel",
    "pipeline": "npm run clean && tsc && npm run babel && npm run webpack && npm run clean-build-steps",

    "ts": "tsc",
    "test": "echo \"Error: no test specified\" && exit 1",
  
    "ALT_prebuild": "rimraf ./build",
    "ALT_postbuild": "cpy **/* !**/*.ts ../build --cwd=./src --parents",
    "ALT_pipeline": "npm run prebuild && tsc && npm run postbuild",
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.1",
    "@babel/node": "^7.10.5",
    "@babel/preset-env": "^7.11.0",
    "@types/lodash": "^4.14.159",
    "@types/react": "^16.9.46",
    "@types/react-dom": "^16.9.8",
    "@typescript-eslint/eslint-plugin": "^3.9.0",
    "@typescript-eslint/parser": "^3.9.0",
    "cpy-cli": "^3.1.1",
    "eslint": "^7.6.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-standard": "^4.0.1",
    "nodemon": "^2.0.4",
    "rimraf": "^3.0.2",
    "typescript": "^3.9.7",
    "webpack": "^4.44.2"
  },
  "author": "Daniel Hoop & Mehrdad Zendehzaban",
  "license": "UNLICENSED"
}
